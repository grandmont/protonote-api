    "express": "^4.18.2",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.17.0",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "node-fetch": "2",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.0.3",
    "prisma": "3.15",
    "prisma-dbml-generator": "^0.9.1",
    "ts-node-dev": "^2.0.0",
    "typegraphql-prisma": "^0.20.4",
    "typescript": "^4.7.4"
