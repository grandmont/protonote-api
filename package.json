{
  "name": "prisma-typegraphql",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "scripts": {
    "dev": "yarn docker && ts-node-dev --no-notify --respawn --transpile-only src",
    "start": "node dist/server",
    "docker": "docker-compose up -d",
    "build": "yarn generate && tsc",
    "grc": "node ./scripts grc",
    "migrate": "prisma migrate dev",
    "generate": "yarn generate:prisma",
    "generate:prisma": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "3.15",
    "@types/graphql-fields": "^1.3.4",
    "apollo-server": "^3.9.0",
    "class-validator": "^0.13.2",
    "graphql-fields": "^2.0.3",
    "graphql-scalars": "^1.17.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^18.0.3",
    "prisma": "3.15",
    "prisma-dbml-generator": "^0.9.1",
    "ts-node-dev": "^2.0.0",
    "typegraphql-prisma": "^0.20.4",
    "typescript": "^4.7.4"
  }
}
