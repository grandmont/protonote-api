generator client {
  provider = "prisma-client-js"
}

generator typegraphql {
  provider = "typegraphql-prisma"
  output   = "type-graphql"
}

generator dbml {
  provider = "prisma-dbml-generator"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  name     String?
  password String?
}

model Proto {
  id   Int     @id @default(autoincrement())
  name String?
}

model ProtoProp {
  id   Int           @id @default(autoincrement())
  name String?
  type ProtoPropType
}

enum ProtoPropType {
  DESCRIPTION
  LIKERT
}
